class Solution{
  public:
    int MissingNumber(vector<int>& array, int n) {
        // Your code goes here
           int sum=0,total;

        for(int i=1;i<=n;i++){

            sum=sum+i;

        }

        for(int i=0;i<n-1;i++){

            total=total+array[i];

        }

        return sum-total;

    }
};
